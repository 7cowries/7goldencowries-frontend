{
  "rewrites": [
    {
      "source": "/auth/:match*",
      "destination": "https://sevengoldencowries-backend.onrender.com/auth/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "https://sevengoldencowries-backend.onrender.com/api/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.7goldencowries.com/:path*",
      "permanent": true
    }
  ]
}
