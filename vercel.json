{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "CI= npm run build",
  "outputDirectory": "build",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://sevengoldencowries-backend.onrender.com/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
